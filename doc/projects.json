            {
                "name": "name",
                "description": "Jenkins project name.",
                "paramType": "path",
                "required": true,
                "allowMultiple": false,
                "dataType": "string"
            }
          ],
          "errorResponses": [
            {
              "code": 404,
              "reason": "Not found"
            }
          ]
        }
      ]
    }
  ],
  "models": {
